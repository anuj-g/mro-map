Map {
  background-color: #68deff;
}

#countries {
  ::outline {
    line-color: #85c5d3;
    line-width: 2;
    line-join: round;
  }
  polygon-fill: #ccc;
}

#bangladeshdivisions {
  polygon-fill: #fff;
}

#bangladeshdivisions[name="Dhaka"] {
  polygon-fill: #fbb;
}
#bangladeshdivisions[name="Sylhet"] {
  polygon-fill: #caf;
}
#bangladeshdivisions[name="Khulna"] {
  polygon-fill: #bbf;
}
#bangladeshdivisions[name="Barisal"] {
  polygon-fill: #bfb;
}
#bangladeshdivisions[name="Chittagong"] {
  polygon-fill: #fca;
}
#bangladeshdivisions[name="Rajshahi"] {
  polygon-fill: #fed;
}
#bangladeshdivisions[name="Rangpur"] {
  polygon-fill: #dfe;
}

#bangladeshdivisions {
  line-width:1;
  line-color:#168;
  text-name: "[name_local]";
  text-face-name: "Mro Unicode Regular";
  text-allow-overlap: true;
  text-fill: #000;
  text-size: 16;
  text-halo-fill: #fff;
  text-halo-radius: 3;
}